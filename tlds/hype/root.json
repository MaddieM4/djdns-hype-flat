{
    "meta": {
        "about": "The DJDNS document for the .hype TLD."
    },
    "branches": [
        {
            "selector": "(^|\\.)ri\\.hype$",
            "records" : [],
            "targets" : ["private/ri.json"]
        },
        {
            "selector": "^(appnode|gitboria|hypeirc|hyperboria|projectmeshnet|socialnode|urlcloud)\\.hype$",
            "records" : [],
            "targets" : ["private/dansup.json"]
        },
        {
            "selector": "^riddims\\.hype$",
            "records" : [],
            "targets" : ["private/liam.json"]
        },
        {
            "selector": ".*",
            "records" : [],
            "targets" : ["tlds/hype/generated.json"]
        }
    ]
}
