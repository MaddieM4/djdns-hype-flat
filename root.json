{
    "branches": [
        {
            "selector": "\\.hype$",
            "records" : [],
            "targets" : ["./tlds/hype/root.json"]
        },
        {
            "selector": "\\.bit$",
            "records" : [],
            "targets" : ["defer/namecoin.json"],
            "comment" : "Namecoin (.bit)"
        },
        {
            "selector": "\\.(bbs|dyn|free|fur|geek|gopher|indy|ing|micro|neo|null|oss|oz|parody)$",
            "records" : [],
            "targets" : ["defer/opennic.json"],
            "comment" : "OpenNIC domains"
        },
        {
            "selector": ".*",
            "records" : [],
            "targets" : ["defer/icann.json"],
            "comment" : "ICANN domains"
        }
    ]
}
